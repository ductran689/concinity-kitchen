generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model About {
  id   String @id @default(auto()) @map("_id") @db.ObjectId
  img  String
  name String
}

model Gallery {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model services {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  content String
  image   String
  name    String
  slug    String
}

model top_img {
  id   String @id @default(auto()) @map("_id") @db.ObjectId
  img  String
  name String
}
